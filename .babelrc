{
    "presets": [
      "@babel/preset-typescript",
      [
        "@babel/preset-env",
        {
          "modules": false,
          "useBuiltIns": "usage",
          "corejs": "2.6.10",
          "targets": {
            "ie": 10
          }
        }
      ]
    ],
    "plugins": [
        ["@babel/plugin-transform-runtime"]
    ],
    "ignore": [
        "node_modules/**"
      ]
  }